{
  "expo": {
    "name": "FixIt",
    "slug": "fixit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fixit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-image-picker",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "59e9af4e-220b-4a5b-8991-94b3ddf81039"
      }
    },
    "android": {
      "package": "com.FixIt.FirstPrototype",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "owner": "TheArchitect"
  }
}
